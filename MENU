let DataPack = require('AQUABOT-MD');
let SewQueen = require('AQUABOT-MD/sources/dc/handler');
let Details = require('AQUABOT-MD/sources/dc/Details');
let {sendMessageAllMenupv,sendMessageAllMenupb, sendMessageonemenupv,sendMessageonemenupb } = require('AQUABOT-MD/sources/dc/cmd/menu')
let WorkType = Details.WORKTYPE == 'public' ? false : true
AQUABOT-MD['IntroduceCMD']({
        pattern: 'help ?(.*)', 
        fromMe: false, 
        dontAdCommandList: true,
        }, 
(async (QueenSew,input) => { 
 if(Details.WORKTYPE !== 'public') return;
await sendMessageAllMenupb(AQUABOT-MD,input)
})); 
